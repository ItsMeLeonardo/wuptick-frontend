{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "env": {
        "es6": true,
        "node": true,
        "mocha": true,
        "browser": true
    },
    "globals": {
        "ReactDOM": true
    },
    "rules": {
        "no-console": "warn"
    }
}